            "> 1%",
            "last 2 versions",
            "not ie <= 11"
          ]
        },
        "useBuiltIns": true,
        "modules": "commonjs"
      }
    ],
    "react"
  ],
  "plugins": [
    "add-module-exports"
  ]
}
