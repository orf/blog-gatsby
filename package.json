{
  "name": "blog-gatsby",
  "description": "Toms Gatsby Blog",
  "version": "1.0.0",
  "author": "Tom Forbes <tom@tomforb.es>",
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not ie <= 11"
  ],
  "dependencies": {
    "gatsby": "1.9.251",
    "gatsby-image": "1.0.46",
    "gatsby-link": "1.6.40",
    "gatsby-plugin-canonical-urls": "1.0.16",
    "gatsby-plugin-favicon": "2.1.1",
    "gatsby-plugin-feed": "1.3.20",
    "gatsby-plugin-google-analytics": "1.0.30",
    "gatsby-plugin-manifest": "1.0.19",
    "gatsby-plugin-netlify": "1.0.19",
    "gatsby-plugin-nprogress": "1.0.14",
    "gatsby-plugin-react-helmet": "2.0.10",
    "gatsby-plugin-remove-serviceworker": "^1.0.0",
    "gatsby-plugin-sharp": "1.6.42",
    "gatsby-plugin-sitemap": "1.2.21",
    "gatsby-plugin-typography": "1.7.18",
    "gatsby-remark-autolink-headers": "1.4.16",
    "gatsby-remark-copy-linked-files": "1.5.31",
    "gatsby-remark-images": "1.5.61",
    "gatsby-remark-prismjs": "2.0.1",
    "gatsby-remark-responsive-iframe": "1.4.18",
    "gatsby-remark-sequence": "0.0.1",
    "gatsby-remark-smartypants": "1.4.12",
    "gatsby-source-filesystem": "1.5.33",
    "gatsby-transformer-remark": "1.7.40",
    "gatsby-transformer-sharp": "1.6.22",
    "lodash": "^4.17.4",
    "prismjs": "1.14.0",
    "react-gravatar": "2.6.3",
    "react-helmet": "^5.2.0",
    "react-responsive-grid": "^0.3.4",
    "typography-theme-us-web-design-standards": "^0.16.9"
  },
  "devDependencies": {
    "babel-core": "^6.26.0",
    "babel-preset-env": "1.6.1",
    "eslint": "^4.15.0",
    "eslint-plugin-babel": "^5.0.0",
    "eslint-plugin-import": "^2.8.0",
    "eslint-plugin-jsx-a11y": "^6.0.3",
    "eslint-plugin-react": "^7.5.1",
    "gatsby-plugin-remove-trailing-slashes": "^1.0.3",
    "gh-pages": "^1.1.0",
    "prettier": "^1.10.1"
  },
  "homepage": "https://github.com/orf/blog-gatsby",
  "keywords": [
    "gatsby"
  ],
  "license": "MIT",
  "main": "n/a",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/orf/blog-gatsby.git"
  },
  "scripts": {
    "dev": "gatsby develop",
    "lint": "./node_modules/.bin/eslint --ext .js,.jsx --ignore-pattern public .",
    "test": "echo \"Error: no test specified\" && exit 1",
    "format": "prettier --trailing-comma es5 --no-semi --single-quote --write 'src/**/*.js' 'src/**/*.md'",
    "develop": "gatsby develop",
    "build": "gatsby build",
    "deploy": "gatsby build --prefix-paths && gh-pages -d public",
    "fix-semi": "eslint --quiet --ignore-pattern node_modules --ignore-pattern public --parser babel-eslint --no-eslintrc --rule '{\"semi\": [2, \"never\"], \"no-extra-semi\": [2]}' --fix gatsby-node.js"
  }
}
