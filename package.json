{
  "name": "blog-gatsby",
  "description": "Toms Gatsby Blog",
  "version": "1.0.0",
  "author": "Tom Forbes <tom@tomforb.es>",
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not ie <= 11"
  ],
  "dependencies": {
    "gatsby": "1.9.209",
    "gatsby-image": "^1.0.33",
    "gatsby-link": "^1.6.34",
    "gatsby-plugin-canonical-urls": "^1.0.11",
    "gatsby-plugin-catch-links": "^1.0.14",
    "gatsby-plugin-favicon": "^2.1.1",
    "gatsby-plugin-feed": "^1.3.15",
